{{#partial "content"}}

    <div class="container">
        <div class="row">
            <div class="col-md-6 transparent">
                <h2>Squads</h2>
                <ul class="list-group display-list">

                    {{#each squads}}
                        <li class="list-group-item">{{name}}</li>
                    {{/each}}
                </ul>
            </div>
            <div class="col-md-6 ">
                <h2>Heroes without Squads</h2>
                <ul class="list-group display-list ">

                    {{#each heroesWithoutSquads}}
                        <li class="list-group-item transparent">{{name}}</li>
                    {{/each}}
                </ul>
            </div>
        </div>
    </div>


{{/partial}}

{{> layout.hbs}}